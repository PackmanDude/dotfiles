# Colours
alias diff='diff -r --color=auto'
alias dir='dir --color=auto'
alias ls='ls -F --color=auto'
alias vdir='vdir --color=auto'
# Grep
alias egrep='egrep --color=auto'
alias fgrep='fgrep --color=auto'
alias grep='grep --color=auto'
# Relocate files
alias cp='cp -aiv'
alias mv='mv -iv'
alias rm='rm -Iv'
alias rmdir='rmdir -v'
# File attributes
alias chmod='chmod -v'
alias chown='chown -v'
# List files
alias lA='ls -lA'
alias la='ls -la'
alias lh='ls -lh'
alias lhA='ls -lhA'
alias lha='ls -lha'
alias ll='ls -l'
# Miscellaneous
alias ..='cd ..'
alias cls='clear'
alias gallery-dl='gallery-dl -D .'
alias gcc='gcc -std=c99 -Wall -Wextra -Wpedantic -Wformat=2 -Wshadow \
	-Wwrite-strings -Wstrict-prototypes -Wold-style-definition \
	-Wredundant-decls -Wnested-externs -Wmissing-include-dirs \
	-Wjump-misses-init -Wlogical-op'
alias ln='ln -v'
alias lolcat='lolcat -t'
alias makepkg='makepkg -sirc'
alias mkdir='mkdir -pv'
alias neofetch='pridefetch -f UPA'
alias pacman='pacman --disable-download-timeout'
alias patool='patool -v'
alias tree='tree -F'
alias vi='vim'
alias vim='vim -p'
alias wget='wget -nc'
alias whoami='echo "I'\''m Travis. Travis Scott."'
alias xxd='xxd -u'
alias youtube-dl='yt-dlp'
alias zip='zip -r'
